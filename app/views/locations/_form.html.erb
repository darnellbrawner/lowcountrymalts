<%= form_for @location, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :club_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :club_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :name, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :name, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :description, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :description, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :address_line1, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :address_line1, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :address_line2, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :address_line2, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :city, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :city, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :state_province, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :state_province, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :postal_code, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :postal_code, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :country_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :country_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :latitude, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :latitude, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :longitude, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :longitude, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :atn1, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :atn1, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :atv1, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :atv1, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :atn2, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :atn2, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :atv2, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :atv2, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :resource_type, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :resource_type, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :resource_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :resource_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :active, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :active, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                locations_path, :class => 'btn' %>
  </div>
<% end %>
